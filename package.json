{
  "name": "@rikishi/svg-scaler-viewbox",
  "version": "0.2.3",
  "description": "SVG scale utility",
  "keywords": [
    "utils",
    "svg",
    "scale",
    "resize"
  ],
  "scripts": {
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build": "npm run build:es && npm run build:cjs",
    "release": "standard-version",
    "lint": "eslint . --ext .js",
    "lint:update": "export PKG=eslint-config-airbnb-base; npm info \"$PKG@latest\" peerDependencies --json | command sed 's/[{},]//g ; s/: /@/g' | xargs yarn add --dev \"$PKG@latest\"",
    "precommit": "lint-staged",
    "security": "nsp check",
    "co": "git-cz"
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=codeframe --fix",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:madzhup/svg-scaler-viewbox.git"
  },
  "bugs": {
    "url": "https://github.com/madzhup/svg-scaler-viewbox/issues"
  },
  "homepage": "https://github.com/madzhup/svg-scaler-viewbox",
  "author": "madzhup <madzhup@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "sax": "1.2.4",
    "svgpath": "2.2.2"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-stage-0": "7.0.0",
    "babel-eslint": "10.0.2",
    "commitizen": "4.0.3",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "3.0.3",
    "lint-staged": "9.2.1",
    "nsp": "3.2.1",
    "prettier": "1.18.2",
    "standard-version": "7.0.0",
    "webpack": "4.39.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
